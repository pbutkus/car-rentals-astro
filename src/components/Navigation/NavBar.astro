---
import { getEntry } from "astro:content";
const identity = await getEntry("site-identity", "site-identity");
---

<div class="sticky bg-zinc-950 text-gray-200">
  <div class="flex flex-row py-4 px-12 justify-between items-center">
    <div>
      <img class="h-10" src={identity.data.logo} alt={identity.data.title} />
    </div>
    <div>
      <ul class="inline-flex space-x-6">
        <li><a href="/">HOME</a></li>
        <li>CARS</li>
        <li>CONTACT</li>
      </ul>
    </div>
  </div>
</div>
